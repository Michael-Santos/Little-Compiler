PROGRAM fibonacci
BEGIN

  FUNCTION FLOAT soma (INT x , INT y )
  BEGIN
    INT z ;
    z := x + y ;
    RETURN z ;
  END

  FUNCTION INT recursao (INT x , INT y , INT num , INT entrada )
  BEGIN
    INT temp1 ;
    IF ( num = entrada ) THEN
        RETURN y ;
    ENDIF
            --Retorna float em uma variavel que Ã© INT
    temp1 := soma ( x , y ) ;
    x := y ;
    y := temp1 ;
    num := num + 1 ;
    y := recursao ( x , y , num , entrada ) ;
    RETURN y ;
  END

  FUNCTION INT main ()
  BEGIN
    INT x , y ,result , num , entrada ;

    READ ( entrada );

    x := 0 ;
    y := 1 ;
    num := 1 ;

    IF ( entrada = 0 ) THEN
        result := x ;
    ELSE
        result := recursao (x , y , num , entrada ) ; 
    ENDIF

    WRITE ( result );
    RETURN 0 ;
  END
END